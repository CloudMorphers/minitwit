@model MiniTwit.Models.Auth.LoginViewModel

<h2>Login</h2>

<form method="post">
    <div>
        <label>Email</label>
        <input type="email" asp-for="Email" />
    </div>
    <div>
        <label>Password</label>
        <input type="password" asp-for="Password" />
    </div>
    <button type="submit">Login</button>
</form>
